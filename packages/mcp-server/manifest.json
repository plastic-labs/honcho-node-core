{
  "dxt_version": "0.2",
  "name": "@honcho-ai/mcp",
  "version": "1.6.1",
  "description": "The official MCP Server for the Honcho API",
  "author": {
    "name": "Honcho",
    "email": "hello@plasticlabs.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plastic-labs/honcho-node-core.git"
  },
  "homepage": "https://github.com/plastic-labs/honcho-node-core/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.honcho.dev",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "HONCHO_API_KEY": "${user_config.HONCHO_API_KEY}"
      }
    }
  },
  "user_config": {
    "HONCHO_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
